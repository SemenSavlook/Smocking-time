{
  "name": "webpack-5-lesson",
  "version": "1.0.0",
  "description": "React-course",
  "main": "index.js",
  "scripts": {
    "dev": "webpack serve --mode development",
    "build": "webpack --mode production"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "browserslist": "> 1.0%, not dead",
  "devDependencies": {
    "@babel/core": "^7.17.3",
    "@babel/preset-react": "^7.16.7",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.6.0",
    "eslint": "^8.39.0",
    "eslint-plugin-react": "^7.32.2",
    "html-webpack-plugin": "^5.5.1",
    "mini-css-extract-plugin": "^2.5.3",
    "style-loader": "^3.3.2",
    "webpack": "^5.69.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  },
  "dependencies": {
    "@babel/eslint-parser": "^7.21.8",
    "countup.js": "^2.6.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "babel": {
    "presets": [
      "@babel/react"
    ]
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
